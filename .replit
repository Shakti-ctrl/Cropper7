modules = ["nodejs-20", "web", "ruby-3.2"]
run = "npm run start"
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["openssh_gssapi"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Educational File Server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "display_alpha_file"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "React Development Server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "React Dev Server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "React App"

[[workflows.workflow]]
name = "React Development Server"
author = "agent"
mode = "parallel"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm start"
waitForPort = 3000

[[workflows.workflow]]
name = "React Dev Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "BROWSER=none npm start"
waitForPort = 3000

[[workflows.workflow]]
name = "React App"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "BROWSER=none HOST=0.0.0.0 PORT=5000 npm start"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 3000
externalPort = 80

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 5000
externalPort = 5000

[deployment]
run = ["sh", "-c", "npm run start"]
